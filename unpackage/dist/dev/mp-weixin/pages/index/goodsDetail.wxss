.body {
  font-size: 32rpx;
  padding-bottom: 120rpx;
}
.body .goodsImage {
  width: 100vw;
  height: 600rpx;
  border-bottom: 1rpx solid #eee;
}
.body .goodsInfoMoudle .title {
  font-size: 34rpx;
  padding: 0 20rpx;
}
.body .goodsInfoMoudle .text {
  padding: 0 20rpx;
  margin: 10rpx 0;
  display: flex;
}
.body .goodsInfoMoudle .text .price {
  color: red;
}
.body .goodsInfoMoudle .otherInfo {
  border: 1rpx solid #eee;
  margin: 10rpx 0;
  padding: 10rpx;
}
.body .tipsMoudle {
  background-color: #eee;
  padding: 20rpx;
  white-space: nowrap;
  width: 95%;
  text-overflow: ellipsis;
  overflow: hidden;
  margin-bottom: 10rpx;
}
.body .functionMoudle .textBox {
  padding: 30rpx;
  padding-right: 120rpx;
  border-top: 1rpx solid #eee;
  font-size: 38rpx;
  background-repeat: no-repeat;
  background-position: 95% center;
}
.body .functionMoudle .textBox .title {
  margin-bottom: 10rpx;
}
.body .functionMoudle .textBox .text {
  font-size: 28rpx;
  color: #666;
}
.body .functionMoudle .textBox:nth-child(1) {
  background-image: url(data:image/png;base64,);
  background-size: 60rpx 60rpx;
}
.body .functionMoudle .textBox:nth-child(2) {
  background-image: url(data:image/png;base64,);
  background-size: 60rpx 60rpx;
}
.body .functionMoudle .textBox:nth-child(3) {
  background-image: url(data:image/png;base64,);
  background-size: 60rpx 60rpx;
  border-bottom: 1rpx solid #eee;
}
.body .commentMoudle {
  padding: 20rpx;
  padding-bottom: 0;
}
.body .commentMoudle .commentTop {
  display: flex;
  justify-content: space-between;
  margin-bottom: 20rpx;
}
.body .commentMoudle .commentBox {
  border-bottom: 1rpx solid #eee;
  padding-bottom: 20rpx;
  margin-bottom: 20rpx;
}
.body .commentMoudle .commentBox .userInfo {
  display: flex;
  align-items: center;
}
.body .commentMoudle .commentBox .userInfo .avatar {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 10rpx;
}
.body .commentMoudle .commentBox:last-child {
  margin-bottom: 0;
  border-bottom: none;
}
.body .bottomMoudle {
  display: flex;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #fff;
  justify-content: space-between;
  height: 80rpx;
  padding: 20rpx;
  align-items: center;
  border-top: 1rpx solid #eee;
}
.body .bottomMoudle .leftBox, .body .bottomMoudle .centerBox, .body .bottomMoudle .rightBox {
  display: flex;
  font-size: 24rpx;
}
.body .bottomMoudle .leftBox .text:nth-child(1) {
  color: #000;
  padding-top: 40rpx;
  margin-right: 30rpx;
  background-image: url(data:image/png;base64,);
  background-position: top center;
  background-repeat: no-repeat;
  background-size: 40rpx;
}
.body .bottomMoudle .leftBox .text:nth-child(2) {
  color: #000;
  padding-top: 40rpx;
  margin-right: 20rpx;
  background-image: url(data:image/png;base64,);
  background-position: top center;
  background-repeat: no-repeat;
  background-size: 40rpx;
  position: relative;
}
.body .bottomMoudle .leftBox .text:nth-child(2) .badge {
  position: absolute;
  width: 30rpx;
  line-height: 30rpx;
  text-align: center;
  top: -8rpx;
  right: 0;
  font-size: 18rpx;
  border-radius: 50%;
  background-color: #f56c6c;
  color: #fff;
}
.body .bottomMoudle .rightBox view {
  line-height: 78rpx;
  text-align: center;
  border-radius: 100rpx;
  font-size: 34rpx;
  width: 260rpx;
  color: #fff;
  font-weight: 600;
}
.body .bottomMoudle .rightBox .addButton {
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
  background: linear-gradient(to right, #ffca00, #ff9502);
}
.body .bottomMoudle .rightBox .buyButton {
  border-top-left-radius: 0;
  border-bottom-left-radius: 0;
  background: linear-gradient(to right, #ff7600, #ff5300);
}

